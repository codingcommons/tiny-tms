{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"incremental": true,
		"emitDeclarationOnly": true,
		"noEmit": true
	},
	"include": [
		"src/**/*.d.ts",
		"src/**/*.js",
		"src/**/*.ts",
		"src/**/*.svelte",
		"tests/**/*.ts",
		"services/**/*.ts",
		"./*.ts",
		"./*.js",
		"services",
		"docs/.vitepress/config.ts"
	]
}
