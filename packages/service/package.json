{
	"name": "@repo/service",
	"version": "0.0.0",
	"type": "module",
	"module": "index.ts",
	"main": "index.ts",
	"exports": {
		".": {
			"types": "./index.ts",
			"svelte": "./index.ts"
		}
	},
	"scripts": {
		"lint": "eslint .",
		"format": "prettier --write . && eslint --fix .",
		"test:unit": "esbuild src/kysely/migrations/**.ts --outdir=src/kysely/migrations/js && cross-env DATABASE_LOCATION=:memory: vitest run",
		"setupdb": "pnpm migrate && pnpm db-types",
		"migrate": "tsx ./src/kysely/migrator.ts",
		"db-types": "cross-env DATABASE_URL=db.sqlite kysely-codegen"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/node": "^20.12.7",
		"cross-env": "^7.0.3",
		"esbuild": "^0.20.2",
		"kysely-codegen": "^0.15.0",
		"tsx": "^4.7.3",
		"vitest": "^1.3.1"
	},
	"dependencies": {
		"@types/jsonwebtoken": "^9.0.6",
		"better-sqlite3": "^9.6.0",
		"jsonwebtoken": "^9.0.2",
		"kysely": "^0.27.3",
		"zod": "^3.23.5"
	}
}
